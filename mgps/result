const raceResultSchema: Schema<IRaceResult> = new Schema<IRaceResult>(
	{
		race_object_id: {
			...SCHEMA_DEFINITION_PROPERTY.optionalNullObjectId,
			ref: "races"
		},
		race_round_no: SCHEMA_DEFINITION_PROPERTY.optionalNullNumber,
		race_date: SCHEMA_DEFINITION_PROPERTY.optionalNullString, // Assuming you have a requiredDate definition
		grade: SCHEMA_DEFINITION_PROPERTY.optionalNullString,
		round: [{
			time: SCHEMA_DEFINITION_PROPERTY.optionalNullDate,
			winners: [{
				position: SCHEMA_DEFINITION_PROPERTY.optionalNullNumber,
				dog_object_id: {
					...SCHEMA_DEFINITION_PROPERTY.optionalNullObjectId,
					ref: "dogs"
				},
				race_completion_time: SCHEMA_DEFINITION_PROPERTY.optionalNullDate,
				mgn: SCHEMA_DEFINITION_PROPERTY.optionalNullNumber,
				split: SCHEMA_DEFINITION_PROPERTY.optionalNullNumber,
				in_run: SCHEMA_DEFINITION_PROPERTY.optionalNullNumber,
				weight: SCHEMA_DEFINITION_PROPERTY.optionalNullNumber,
				dog_price: SCHEMA_DEFINITION_PROPERTY.requiredNumber,
			}],
		}],
	},
