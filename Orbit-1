    public function calculateDistances($pickup, $drop)
    {
        $params = [
            'origins' => "{$pickup['lat']},{$pickup['lng']}",
            'destinations' => implode('|', array_map(function ($drop) {
                return "{$drop['lat']},{$drop['lng']}";
            }, $drop)),
            'key' => config('constants.GOOGLE_MAP_API_KEY'),
            'mode' => 'driving' // or 'walking', 'bicycling', etc.
        ];
        $response = Http::get(config('constants.GOOGLE_MAP_DISTANCE_URL'), $params);

        $distances=$response->json();
        if ($distances) {
            // return $response->json();
            // if ($distances) {
                foreach ($distances['rows'] as $row) {
                    foreach ($row['elements'] as $key => $element) {
                        // Store distance and duration in the array
                        $totalkilometers[$key] = [
                            'distance' => $element['distance']['text'],
                            'duration' => $element['duration']['text']
                        ];
                    }
                }

                // Calculate the total distance
                    $totalDistance = $this->getTotalDistance($distances);
                    // dd($totalDistance['totalDistance'],$totalDistance['totalduration']);
                    $kilometers = $totalDistance['totalDistance'] / 1000; // Convert meters to kilometers
                    $totalduration = $totalDistance['totalduration'] / 60; // Convert meters to kilometers
                    // dd($kilometers,$totalduration);
            // }

            return [
                    'totalDistance' => $kilometers,
                    'totalduration' => $totalduration,
                    'totalkilometers' => $totalkilometers
                ];
        }

        return null; // Handle errors appropriately
    }

    public function getTotalDistance($distances)
    {
        $totalDistance = 0;
        $totalduration = 0;

        foreach ($distances['rows'] as $row) {
            foreach ($row['elements'] as $element) {
                // Convert distance to numeric value (in meters)
                $distanceValue = $element['distance']['value']; // in meters
                $durationValue = $element['duration']['value'];// in meters
                $totalDistance += $distanceValue;
                $totalduration += $durationValue;
            }
        }
    // dd($totalDistance,$totalduration);
        return compact('totalDistance','totalduration'); // Total distance in meters
    }





