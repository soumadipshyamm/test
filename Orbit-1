    private function getETA($latitudeFrom, $longitudeFrom, $latitudeTo, $longitudeTo, $stops = 0)
    {
        // Prepare the request parameters
        $params = [
            'origins' => $latitudeFrom . ',' . $longitudeFrom,
            'destinations' => $latitudeTo . ',' . $longitudeTo,
            'mode' => 'driving',
            'key' => config('constants.GOOGLE_MAP_API_KEY'),
        ];

        // Make the HTTP request to the Google Maps Distance API
        $distanceResponse = Http::get(config('constants.GOOGLE_MAP_DISTANCE_URL'), $params);

        // Decode the JSON response
        $distance = $distanceResponse->json();

        // Check if the response status is OK
        if ($distance['status'] === 'OK') {
            // Check if elements are OK
            if ($distance['rows'][0]['elements'][0]['status'] === 'OK') {
                // Create an object for ETA information
                $eta = (object) [
                    'distance' => $distance['rows'][0]['elements'][0]['distance']['text'],
                    'distance_metar' => $distance['rows'][0]['elements'][0]['distance']['value'],
                    'duration' => $distance['rows'][0]['elements'][0]['duration'],
                    'status' => $distance['rows'][0]['elements'][0]['status'],
                ];

                return $eta; // Return the ETA object
            } else {
                // Handle the case where the element status is not OK
                logger('Element status not OK: ' . json_encode($distance['rows'][0]['elements'][0]));
                return false;
            }
        } else {
            // Handle the case where the overall status is not OK
            logger('Distance API status not OK: ' . json_encode($distance));
            return false;
        }
    }

